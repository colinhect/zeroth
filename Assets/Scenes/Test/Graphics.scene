---
sceneType: Scene
base: Hect/Scenes/Default.scene
componentTypes:
  - ObserverCamera
systemTypes:
  - PlayerInputSystem
  - ObserverCameraSystem
entities:
  - base: Test/Observer.entity
  - components:
    - componentType: LightProbe
      texture: Test/Space.texturecube
    - componentType: SkyBox
      texture: Test/Space.texturecube
    - componentType: DirectionalLight
      direction: [ 1, 0.2, 0.2 ]
      color: [ 0.5, 0.5, 0.5 ]
  - components:
      - componentType: Transform
        localPosition: [ 0, 10, 0 ]
      - componentType: BoundingBox
      - componentType: Model
        surfaces:
          - mesh: Test/Cube.Main.mesh
            material: Test/MetalCrate.material
  - components:
      - componentType: Transform
        localPosition: [ -10, 10, 0 ]
      - componentType: BoundingBox
      - componentType: Model
        surfaces:
          - mesh: Test/Cube.Main.mesh
            material: Test/Dusty.material
  - components:
      - componentType: Transform
        localPosition: [ -15, 10, 0 ]
      - componentType: BoundingBox
      - componentType: Model
        surfaces:
          - mesh: Test/Cube.Main.mesh
            material: Test/Dusty.material
  - components:
      - componentType: Transform
        localPosition: [ 10, 10, 0 ]
      - componentType: BoundingBox
      - componentType: Model
        surfaces:
          - mesh: Test/Cube.Main.mesh
            material: Test/MetalCrate.material
