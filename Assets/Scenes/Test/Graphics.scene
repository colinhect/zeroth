---
sceneType: Scene
base: Hect/Scenes/Default.scene
componentTypes:
  - ObserverCameraComponent
systemTypes:
  - PlayerInputSystem
  - ObserverCameraSystem
entities:
  - base: Test/Observer.entity
  - components:
    - componentType: LightProbeComponent
      texture: Test/Space.texturecube
    - componentType: SkyBoxComponent
      texture: Test/Space.texturecube
    - componentType: DirectionalLightComponent
      direction: [ 1, 0.2, 0.2 ]
      color: [ 0.5, 0.5, 0.5 ]
  - components:
      - componentType: TransformComponent
        localPosition: [ 0, 10, 0 ]
      - componentType: BoundingBoxComponent
      - componentType: ModelComponent
        surfaces:
          - mesh: Test/Cube.Main.mesh
            material: Test/MetalCrate.material
  - components:
      - componentType: TransformComponent
        localPosition: [ -10, 10, 0 ]
      - componentType: BoundingBoxComponent
      - componentType: ModelComponent
        surfaces:
          - mesh: Test/Cube.Main.mesh
            material: Test/Dusty.material
  - components:
      - componentType: TransformComponent
        localPosition: [ -15, 10, 0 ]
      - componentType: BoundingBoxComponent
      - componentType: ModelComponent
        surfaces:
          - mesh: Test/Cube.Main.mesh
            material: Test/Dusty.material
  - components:
      - componentType: TransformComponent
        localPosition: [ 10, 10, 0 ]
      - componentType: BoundingBoxComponent
      - componentType: ModelComponent
        surfaces:
          - mesh: Test/Cube.Main.mesh
            material: Test/MetalCrate.material
