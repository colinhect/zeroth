---
base: Scenes/Base/LocalBase.scene
componentTypes:
  - Planet
  - PlanetPatch
systemTypes:
  - PlanetSystem
systems:
  - systemType: PhysicsSystem
    gravity: [ 0, 0, 0 ]
entities:
  - components:
    - componentType: DirectionalLight
      direction: [ -0.5, -0.5, -0.75 ]
      color: [ 1.5, 1.5, 1.5 ]
  - components:
    - componentType: DirectionalLight
      direction: [ 0.5, 0.5, 0.75 ]
      color: [ 1.5, 1.5, 1.5 ]
  - base: Ships/Indor.entity
    components:
      - componentType: Transform
        localPosition: [ 0, 200, 0 ]
  - components:
      - componentType: Transform
        localPosition: [ 0, 240, 0 ]
      - componentType: BoundingBox
      - componentType: Model
        surfaces:
          - mesh: Test/Cube.Main.mesh
            material: Test/MetalCrate.material
      - componentType: RigidBody
        mass: 1
        mesh: Test/Cube.Main.mesh
  - base: Test/Observer.entity
    components:
      - componentType: Transform
        localPosition: [ 0, -250, 0 ]
  - components:
      - componentType: Planet
        name: Endromin
        meanRadius: 50
        patchResolution: 16
        patchMaterial: Hect/Materials/OpaqueSolid.material
